.box-info-div {
    height: 35px;
    padding: 10px;
    background-color: #eeeeee;
    margin-bottom: 5px;
}

.btn-order-detail {
    height: 45px;
    text-align: center;
    background-color: #fcfcfc;
    color: #404040;
    font-size: 1.2em;
    line-height: 45px;
    position: fixed;
    bottom: 10px;
    left: 50px;
    right: 50px;
    z-index: 999;
    cursor: pointer;
    border: 1px solid #e4e4e4
}

.content-order-detail {
    height: 150px;
    background-color: #f3f3f3;
    color: #404040;
    font-size: 1.0em;
    /*line-height: 45px;*/
    position: fixed;
    bottom: 100px;
    left: 10px;
    right: 10px;
    z-index: 999;
    cursor: pointer;
    border-radius: 5px;
}